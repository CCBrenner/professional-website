    protected override void OnInitialized()
    {
        // For setting the NavButton highlights to what they need to be upon first 
        // load in absence of setting SectionId parameter
        if (SectionId != 0)
            NavMgmt.Instance.NavigateToSection(SectionId);
        else
            NavMgmt.Instance.NavigateToSection(17);

        // For dynamic reload of component when state has changed in the following singletons
        NavMgmt.Instance.OnNavMgmtUpdated += UpdateThisComponent;
        SectionMgmt.Instance.OnSectionMgmtChanged += UpdateThisComponent;
    }
    void IDisposable.Dispose()
    {
        NavMgmt.Instance.OnNavMgmtUpdated -= UpdateThisComponent;
        SectionMgmt.Instance.OnSectionMgmtChanged -= UpdateThisComponent;
    }
    private void UpdateThisComponent(string meaninglessRequirement) =>
        StateHasChanged();

    [Parameter]
    public int SectionId { get; set; }

    private void UpdateWorkHistorySectionContent(int selection)
    {
        for (int i = 0; i < SelectedWorkHistoryContent.Count(); i++)
            SelectedWorkHistoryContent[i] = "btn-secondary";
        SelectedWorkHistoryContent[selection] = "btn-primary";
    }    
}
