<div class="panel-container @PanelPosition @NavPanelStatus">
    <nav class="panel">
        @ChildContent
    </nav>
</div>

@code {
    [Parameter]
    public RenderFragment? ChildContent { get; set; }
    [Parameter]
    public string NavPanelStatus { get; set; }
    [Parameter]
    public string PanelPosition { get; set; }
}
